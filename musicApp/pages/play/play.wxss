page{
  background-color: aquamarine;
}
.img{
  width: 700rpx;
  height: 700rpx;
  border:20rpx solid rgba(0,0,0,0.5);
  margin: 25rpx;
  box-sizing: border-box;
}
.wrap{
  overflow: hidden;
}

.now,
.duration{
  font-size: 14px;
  color: #ccc;
}

.now{
  float: left;
}
.duration{
  float: right;
}

.scroll{
  height: 80rpx;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 250rpx;
}
.white{
  color:#fff;
}